{
  "name": "mern-starter",
  "version": "1.0.0",
  "description": "mern starter application",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "concurrently \"npm:server:dev\" \"npm:client:dev\"",
    "server:dev": "NODE_ENV=development nodemon -r dotenv/config index.js",
    "client:build": "NODE_ENV=production webpack --config ./_CONFIG/webpack.config.js",
    "client:dev": "webpack serve --config ./_CONFIG/webpack.config.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airgead73/mern-starter.git"
  },
  "keywords": [
    "mongodb",
    "express",
    "react",
    "node"
  ],
  "author": "dan moneypenny",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/airgead73/mern-starter/issues"
  },
  "homepage": "https://github.com/airgead73/mern-starter#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.4.1",
    "http-errors": "^1.7.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.11.15"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.0.0-beta2",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "html-webpack-plugin": "^5.0.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "sass": "^1.32.6",
    "sass-loader": "^11.0.0",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
